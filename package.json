{
  "name": "bohr",
  "version": "0.0.0",
  "description": "The deploy platform for developers.",
  "author": "bohr.io",
  "bin": {
    "bohr": "./bin/run"
  },
  "homepage": "https://bohr.io",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bohr-io/cli.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^7",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "chai": "^4",
    "eslint": "^7",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "bohr",
    "dirname": "bohr",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "postbuild": "oclif readme",
    "posttest": "npm run lint"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/bohr-io/cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
