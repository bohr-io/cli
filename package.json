{
  "name": "bohr",
  "version": "1.0.367",
  "description": "The deploy platform for developers.",
  "author": "bohr.io",
  "bin": {
    "bohr": "./bin/run"
  },
  "homepage": "https://bohr.io",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bohr-io/cli.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/utils"
  ],
  "dependencies": {
    "@netlify/zip-it-and-ship-it": "^5.13.1",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-warn-if-update-available": "^2.0.4",
    "@vercel/fun": "^1.0.3",
    "axios": "^0.24.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "conf": "^10.1.2",
    "current-git-branch": "^1.1.0",
    "dotenv": "^16.0.1",
    "engine.io-parser": "^5.0.4",
    "express": "^4.17.2",
    "git-remote-origin-url": "^4.0.0",
    "graceful-fs": "^4.2.8",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.4",
    "open": "^8.4.0",
    "ora": "^5.4.1",
    "portfinder": "^1.0.28",
    "supports-color": "^9.2.2",
    "uuid": "^8.3.2",
    "wait-port": "^0.2.9",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/current-git-branch": "^1.1.2",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16",
    "@types/ws": "^8.5.3",
    "eslint": "^7",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "bohr",
    "dirname": "bohr",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>.\nUpdate through your <%= chalk.blueBright('package manager') %>.\n"
    },
    "topicSeparator": " "
  },
  "scripts": {
    "build-cli": "shx rm -rf tmp && rm -rf dist && npx tsc -b && npx oclif manifest && npx oclif readme",
    "lint-cli": "eslint . --ext .ts --config .eslintrc",
    "test-cli": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest-cli": "yarn run lint"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": "https://github.com/bohr-io/cli/issues",
  "keywords": [
    "bohr.io",
    "deploy"
  ],
  "types": "dist/index.d.ts"
}